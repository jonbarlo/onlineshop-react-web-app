{
  "name": "onlineshop-react-web-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && cross-env NODE_ENV=production vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "deploy": "tsx scripts/deploy-ftp.ts",
    "deploy:staging": "tsx scripts/deploy-ftp.ts --environment staging",
    "deploy:production": "tsx scripts/deploy-ftp.ts --environment production",
    "deploy:quick": "tsx scripts/deploy-ftp.ts --skip-build",
    "deploy:dry": "tsx scripts/deploy-ftp.ts --dry-run",
    "deploy:verbose": "tsx scripts/deploy-ftp.ts --verbose"
  },
  "dependencies": {
    "@types/react-color": "^3.0.13",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^15.7.3",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "basic-ftp": "^5.0.5",
    "cross-env": "^10.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.20.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
